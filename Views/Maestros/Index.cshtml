@model List<Mantenimiento.Models.ViewModels.MaestroViewlModel>
@{
    ViewBag.Title = "Maestros";
}

<h2>@ViewBag.Title</h2>

<div class="row">
   <div class="row" style="text-align:right;padding:5px">
        <a href="~/Maestros/Nuevo" class="btn btn-info">Nuevo</a>
   </div>
       <div class="col-md-12">
           <table class="table">
               <tr>
                   <th># </th>
                   <th>Nombre </th>
                   <th>Apellido </th>
               </tr>
               @foreach (var oElemento in Model)
               {
           <tr>
               <td>@oElemento.id </td>
               <td>@oElemento.nombre </td>
               <td>@oElemento.apellido </td>
           </tr>
               }
           </table>
       </div>
</div>

